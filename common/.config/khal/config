[calendars]

[[bart_calendar_local]]
path = ~/.calendars/*
type = discover

[[bart_contacts_local]]
path = ~/.contacts/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = default
highlight_event_days = True
timedelta = 365 days
print_new = event

[view]
theme = light
event_view_always_visible = True
